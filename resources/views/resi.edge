<script> 

var element = document.getElementById("barcode");
JsBarcode(element, "{{a.trnNoHAWB}}");

n =  new Date();
y = n.getFullYear();
m = n.getMonth() + 1;
d = n.getDate();
h = n.getHours();
mn = n.getMinutes();
s = n.getSeconds();



document.getElementById("date").innerHTML = m + "/" + d + "/" + y + "&nbsp;" + h + ":" + mn + ":" + s;
document.getElementById("date1").innerHTML = m + "/" + d + "/" + y + "&nbsp;" + h + ":" + mn + ":" + s;
